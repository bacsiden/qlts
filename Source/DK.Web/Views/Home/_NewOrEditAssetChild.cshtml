@model DK.Application.Models.TaiSan

<div class="modal-dialog modal-xl" role="document">
    <div class="modal-content">
        @using (Html.BeginForm(null, null, FormMethod.Post, new { role = "form", id = "frmAsset" }))
        {
            <div class="modal-header pd-y-20 pd-x-25">
                <h6 class="tx-14 mg-b-0 tx-uppercase tx-inverse tx-bold">Chỉnh sửa tài sản</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body pd-25">
                @Html.Partial("_NewOrEditAssetForm", Model)
            </div>
            <div class="modal-footer">
                <button class="btn  btn-sm bd bg-primary tx-white pointer" onclick="return submitNewOrEditTaiSanCon();"><i class="fa fa-save"></i> Lưu lại</button>
            </div>
        }
    </div>
</div>